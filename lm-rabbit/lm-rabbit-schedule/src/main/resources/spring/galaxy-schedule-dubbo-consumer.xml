    <!-- trade -->
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.rabbit.order.service.OrderSettlementReadService" id="orderSettlementReadService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.rabbit.order.service.OrderSettlementWriteService" id="orderSettlementWriteService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.order.service.OrderReadService" id="orderReadService" check="false"/>
