<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.config.service.ConfigReadService" id="configReadService" check="false"/>


    <!-- pay service -->
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.pay.service.PayWriteService" id="payWriteService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.pay.service.PayReadService" id="payReadService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.pay.mock.service.MockPayReadService" id="mockPayReadService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.pay.mock.service.MockPayWriteService" id="mockPayWriteService" check="false"/>
    <dubbo:reference version="1.0.0" retries="0" interface="cn.blmdz.wolf.pay.mock.service.MockedAlipayTransService" id="mockedAlipayTransService" check="false"/>

    <!-- settlement -->
