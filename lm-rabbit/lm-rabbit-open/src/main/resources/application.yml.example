logging:
  file: ${user.home}/logs/galaxy/open.log
  level.*: INFO
  level.io.terminus: DEBUG

server:
  context-path: /
  port: 8082

openplatform:
  mode: GATEWAY

spring:
  messages:
    basename: messages,op/messages,user/messages
    cache-seconds: -1
    encoding: UTF-8
    fallback-to-system-locale: true

session:
  cookie-domain: api.galaxy.dev
  cookie-context-path: /
  cookie-name: msid
  cookie-max-age: 1800
  source: redis
  serialize-type: json
  redis-host: 127.0.0.1
  redis-port: 6379
  redis-index: 1
  redis-cluster: false
  redis-test-on-borrow: true
  redis-max-total: 10
  redis-max-idle: 0
  redis-prefix: afsession
  mode: manager

zookeeper:
  host: 127.0.0.1
  port: 2181

dubbo:
  name: galaxy-open
  registry: zookeeper://127.0.0.1:2181
  port: -1
  timeout: 10000
  threads: 100
  heartBeats: 10000
  mode: consumer

image:
  base:
    url: http://terminus.oss-cn-hangzhou.aliyuncs.com

oss:
  endpoint: oss-cn-hangzhou.aliyuncs.com
  appKey: ~~removed~~
  appSecret: ~~removed~~
  bucketName: terminus

msg:
  current:
    smsService: aliSmsService
  alisms:
    smsUrl: http://gw.api.taobao.com/router/rest
    version: 2.0
    appKey: ~~removed~~
    appSecret: ~~removed~~
  test:
    enabled: true

parana:
  config:
    mode: server
